package pa1;

public class Client {

	public static void main(String[] args) {
		IContainerStorage containers = new ContainerStack();
		
		containers.addContainer(new ShippingContainer("BOS01"));
		containers.addContainer(new ShippingContainer("BOS02"));
		containers.addContainer(new ShippingContainer("BOS03"));
		
		System.out.println(containers.getContainersToDest("BOS"));
	}

}
